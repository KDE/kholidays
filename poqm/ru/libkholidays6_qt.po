# Или это "выберите, какой регион использовать"?
#~ msgid "<p>Select to use Holiday Region</p>"
#~ msgstr "<p>Выберите нужный регион со своими праздниками</p>"

#~ msgid ""
#~ "<p><b>Region:</b> %1<br/><b>Language:</b> %2<br/><b>Description:</b> %3</"
